    <header>
      <tool tool-id="symfony" tool-name="Symfony"/>
    </header>
    <body>
      <trans-unit id="app.deleteAccount.confirmationEmail.subject" approved="yes">
        <source>app.deleteAccount.confirmationEmail.subject</source>
        <target>Confirmation of profile deletion</target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.confirmationEmail.content" approved="yes">
        <source>app.deleteAccount.confirmationEmail.content</source>
        <target><![CDATA[Your account will be deleted in %daysBeforeDeleting% days, <br> Click <a href="%confirmationLink%">here</a> to confirm this process ]]></target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.informCountryManager.subject" approved="yes">
        <source>app.deleteAccount.informCountryManager.subject</source>
        <target>The user %userEmail% wants to delete their profile</target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.informCountryManager.content" approved="yes">
        <source>app.deleteAccount.informCountryManager.content</source>
        <target>User(%userEmail%) has deleted their profile. Their data will be deleted on %deleteDate%</target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.informUserBeforeExpiration.subject" approved="yes">
        <source>app.deleteAccount.informUserBeforeExpiration.subject</source>
        <target>Sorry to hear you are leaving us.</target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.informUserBeforeExpiration.content" approved="yes">
        <source>app.deleteAccount.informUserBeforeExpiration.content</source>
        <target>Sorry to hear you are leaving us. To reactivate your account log in before %expirationDate%</target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.emailInExpirationDate.subject" approved="yes">
        <source>app.deleteAccount.emailInExpirationDate.subject</source>
        <target>Your profile has been deleted</target>
      </trans-unit>
      <trans-unit id="app.deleteAccount.emailInExpirationDate.content" approved="yes">
        <source>Your account has been deleted. If you want to join, register any time.&lt;br&gt; &lt;a href="%linkToTT%"&gt;Transport Talent website&lt;/a&gt;</source>
        <target>Your account has been deleted. If you want to join, register any time.&lt;br&gt; &lt;a href="%linkToTT%"&gt;TransportTalent website&lt;/a&gt;</target>
      </trans-unit>
    </body>
