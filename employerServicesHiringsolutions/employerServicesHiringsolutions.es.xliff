      <trans-unit id="20e7d10f86ae4912a94d144252b46074" resname="app.employerServicesHiringsolutions.section.singleJobPost.header" approved="yes">
        <source>app.employerServicesHiringsolutions.section.singleJobPost.header</source>
        <target state="translated">Nuestras tarifas son transparentes, así puede saber exactamente lo que paga</target>
