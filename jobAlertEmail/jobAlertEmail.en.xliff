      <trans-unit id="app.jobAlertEmail.jobAlertDeleted" approved="yes">
        <source>app.jobAlertEmail.jobAlertDeleted</source>
        <target state="translated">Dear %firstName%,&lt;br&gt;&lt;br&gt;Your job alert "%title%" has been deactivated. We hope that you have found the position you were looking for. New job alerts can always be set up in your &lt;a href="%jobAlertsUrl%"&gt;profile&lt;/a&gt;. Please let us know if there is anything we can improve about our job alerts to further support your search.&lt;br&gt;&lt;br&gt;Kind regards,&lt;br&gt;&lt;br&gt;Your team at Transport Talent</target>
      </trans-unit>
      <trans-unit id="app.jobAlertEmail.jobAlertDeleted.subject" approved="yes">
        <source>app.jobAlertEmail.jobAlertDeleted.subject</source>
        <target state="translated">Your job alert has been deactivated</target>
