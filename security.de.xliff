  <file source-language="en" datatype="plaintext" original="file.ext" target-language="de">
    <body>
      <trans-unit id="1" approved="yes">
        <source><![CDATA[An authentication exception occurred.]]></source>
        <target><![CDATA[Es ist ein Fehler bei der Authentifikation aufgetreten.]]></target>
      </trans-unit>
      <trans-unit id="2" approved="yes">
        <source><![CDATA[Authentication credentials could not be found.]]></source>
        <target><![CDATA[Es konnten keine Zugangsdaten gefunden werden.]]></target>
      </trans-unit>
      <trans-unit id="3" approved="yes">
        <source><![CDATA[Authentication request could not be processed due to a system problem.]]></source>
        <target><![CDATA[Die Authentifikation konnte wegen eines Systemproblems nicht bearbeitet werden.]]></target>
      </trans-unit>
      <trans-unit id="4" approved="yes">
        <source><![CDATA[Invalid credentials.]]></source>
        <target><![CDATA[Fehlerhafte Zugangsdaten.]]></target>
      </trans-unit>
      <trans-unit id="5" approved="yes">
        <source><![CDATA[Cookie has already been used by someone else.]]></source>
        <target><![CDATA[Cookie wurde bereits von jemand anderem verwendet.]]></target>
      </trans-unit>
      <trans-unit id="6" approved="yes">
        <source><![CDATA[Not privileged to request the resource.]]></source>
        <target><![CDATA[Keine Rechte, um die Ressource anzufragen.]]></target>
      </trans-unit>
      <trans-unit id="7" approved="yes">
        <source><![CDATA[Invalid CSRF token.]]></source>
        <target><![CDATA[Ungültiges CSRF-Token.]]></target>
      </trans-unit>
      <trans-unit id="8" approved="yes">
        <source><![CDATA[Digest nonce has expired.]]></source>
        <target><![CDATA[Digest nonce ist abgelaufen.]]></target>
      </trans-unit>
      <trans-unit id="9" approved="yes">
        <source><![CDATA[No authentication provider found to support the authentication token.]]></source>
        <target><![CDATA[Es wurde kein Authentifizierungs-Provider gefunden, der das Authentifizierungs-Token unterstützt.]]></target>
      </trans-unit>
      <trans-unit id="10" approved="yes">
        <source><![CDATA[No session available, it either timed out or cookies are not enabled.]]></source>
        <target><![CDATA[Keine Session verfügbar, entweder ist diese abgelaufen oder Cookies sind nicht aktiviert.]]></target>
      </trans-unit>
      <trans-unit id="11" approved="yes">
        <source><![CDATA[No token could be found.]]></source>
        <target><![CDATA[Es wurde kein Token gefunden.]]></target>
      </trans-unit>
      <trans-unit id="12" approved="yes">
        <source><![CDATA[Username could not be found.]]></source>
        <target><![CDATA[Der Benutzername wurde nicht gefunden.]]></target>
      </trans-unit>
      <trans-unit id="13" approved="yes">
        <source><![CDATA[Account has expired.]]></source>
        <target><![CDATA[Der Account ist abgelaufen.]]></target>
      </trans-unit>
      <trans-unit id="14" approved="yes">
        <source><![CDATA[Credentials have expired.]]></source>
        <target><![CDATA[Die Zugangsdaten sind abgelaufen.]]></target>
      </trans-unit>
      <trans-unit id="15" approved="yes">
        <source><![CDATA[Account is disabled.]]></source>
        <target><![CDATA[Der Account ist deaktiviert.]]></target>
      </trans-unit>
      <trans-unit id="16" approved="yes">
        <source><![CDATA[Account is locked.]]></source>
        <target><![CDATA[Der Account ist gesperrt.]]></target>
      </trans-unit>
    </body>
